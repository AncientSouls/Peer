{
  "name": "ancient-peer",
  "version": "0.1.0",
  "description": "Friendly core of interaction of peers.",
  "bugs": "https://github.com/AncientSouls/Peer/issues",
  "repository": "AncientSouls/Peer",
  "license": "MIT",
  "scripts": {
    "tsc": "rm -rf ./lib ./tests && tsc ./src/*/*.ts --outDir ./ --sourceMap -d true --module CommonJS",
    "test": "mocha ./tests/index.js --require source-map-support/register",
    "dev-init": "rm -rf .dev && git clone -b dev https://github.com/AncientSouls/AncientSouls.github.io.git .dev && cd .dev && npm run dev-init"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "ancient-mixins": "AncientSouls/Mixins#dev",
    "ancient-cursor": "AncientSouls/Cursor#dev",
    "ancient-channels": "AncientSouls/Channels#dev"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "chai": "^4.1.2",
    "mocha": "^5.0.1",
    "source-map-support": "^0.5.3",
    "typescript": "^2.7.2"
  }
}
